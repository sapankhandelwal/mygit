<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>New_Item_Status_cant_be_Returned</fullName>
    <active>true</active>
    <errorConditionFormula>and( ISNEW(), Text(Status__c)=&#39;Returned&#39; )</errorConditionFormula>
    <errorDisplayField>Status__c</errorDisplayField>
    <errorMessage>New Item cannot have status as [Returned]</errorMessage>
</ValidationRule>
